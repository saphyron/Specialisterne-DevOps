{
  "metadata": [
    {
      "src": [
        {
          "cwd": "..",
          "files": [ "Cereal API/Cereal API/bin/**/net9.0/Cereal API.dll" ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      { "files": [ "index.md", "toc.yml", "articles/**.md" ] },
      { "files": [ "api/**.yml" ], "sidetoc": true }
    ],
    "resource": [ { "files": [ "images/**" ] } ],
    "overwrite": [],
    "dest": "_site",
    "globalMetadata": { "site_name": "Cereal API Docs" },
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "template": [ "default" ]
  }
}
